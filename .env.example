# Environment variables for Playwright tests
# Copy this file to .env.local and fill in the actual values

# Application URL (optional - defaults to production)
# BUILD_URL=https://dash.empirical.run

# Vercel protection bypass (required for running against Vercel deployments)
VERCEL_AUTOMATION_BYPASS_SECRET=your_vercel_bypass_secret_here

# User credentials for authenticated tests
AUTOMATED_USER_EMAIL=your_test_user@example.com
AUTOMATED_USER_PASSWORD=your_test_user_password

# Google login credentials for magic link tests
GOOGLE_LOGIN_EMAIL=your_google_test_user@example.com
GOOGLE_LOGIN_PASSWORD=your_google_password
GOOGLE_LOGIN_AUTH_KEY=your_google_auth_key